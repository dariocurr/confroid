<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".web.RegisterActivity">

    <TextView
            android:layout_width="0dp"
            android:layout_height="23dp" android:id="@+id/username_register_text"
            android:hint="username"
            app:layout_constraintStart_toStartOf="@+id/password_register_text"
            app:layout_constraintEnd_toEndOf="@+id/password_register_text" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="148dp" app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="register"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/register_button"
            app:layout_constraintTop_toTopOf="@+id/back_to_login_button"
            app:layout_constraintBottom_toBottomOf="@+id/back_to_login_button" app:layout_constraintVertical_bias="1.0"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="92dp"
            android:layout_marginRight="92dp"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="24dp" android:id="@+id/password_register_text"
            android:hint="password" app:layout_constraintStart_toStartOf="@+id/back_to_login_button"
            app:layout_constraintEnd_toEndOf="@+id/register_button" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/username_register_text" android:layout_marginTop="16dp"/>
    <Button
            android:text="back"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/back_to_login_button"
            app:layout_constraintTop_toBottomOf="@+id/password_register_text"
            android:layout_marginTop="52dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="92dp" android:layout_marginStart="92dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>